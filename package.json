{
	"name": "cardenas",
	"displayName": "Cardenas",
	"description": "Free templates and algorithms",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cardenas.helloWorld",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cardenas.saveFile",
				"title": "Cardenas: SaveFile"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "search-icon",
					"title": "Cardenas",
					"icon": "media/circle-s-outline.svg"
				}
			]
		},
		"views": {
			"search-icon": [
				{
					"type": "webview",
					"id": "cardenas-full",
					"name": "Cardenas",
					"icon": "media/circle-s-outline.svg",
					"contextualTitle": "Cardenas"
				}
			],
			"explorer": [
				{
					"type": "webview",
					"id": "cardenasPanel",
					"name": "Cardenas"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"btoa": "^1.2.1",
		"form-data": "^3.0.0",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/btoa": "^1.2.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
